doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Historique
  link(rel='preconnect' href='https://fonts.gstatic.com')
  link(href='https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;600;700;800&display=swap' rel='stylesheet')
  link(rel='stylesheet' href='css/bootstrap.css')
  link(rel='stylesheet' href='vendors/simple-datatables/style.css')
  link(rel='stylesheet' href='vendors/perfect-scrollbar/perfect-scrollbar.css')
  link(rel='stylesheet' href='vendors/bootstrap-icons/bootstrap-icons.css')
  link(rel='stylesheet' href='css/app.css')
  link(rel='shortcut icon' href='images/favicon.svg' type='image/x-icon')
#app
  #sidebar.active
    .sidebar-wrapper.active
      .sidebar-header
        .d-flex.justify-content-between
          .logo
            a(href='index.html')
              img(src='images/logo/logo.png' alt='Logo' srcset='')
          .toggler
            a.sidebar-hide.d-xl-none.d-block(href='#')
              i.bi.bi-x.bi-middle
      .sidebar-menu
        ul.menu
          li.sidebar-item
            a.sidebar-link(href='index.html')
              i.bi.bi-grid-fill
              span Acceuil
          li.sidebar-item
            a.sidebar-link(href='Historique.html')
              i.bi.bi-stack
              span Historique
          li.sidebar-item
            a.sidebar-link(href='Consultation.html')
              i.bi.bi-file-earmark-medical-fill
              span Consulter les processus
          li.sidebar-item
            a.sidebar-link(href='Demande.html')
              i.bi.bi-pen-fill
              span Faire une demande
          li.sidebar-item.active
            a.sidebar-link(href='Encours.html')
              i.bi.bi-collection-fill
              span Demandes en cours
      button.sidebar-toggler.btn.x
        i(data-feather='x')
  #main
    header.mb-3
      a.burger-btn.d-block.d-xl-none(href='#')
        i.bi.bi-justify.fs-3
    .page-heading
      .page-title
        .row
          .col-12.col-md-6.order-md-1.order-last
            h3 Les demandes en cours
            p.text-subtitle.text-muted Consulter vos demandes en cours, et faite des modifications
          .col-12.col-md-6.order-md-2.order-first
            nav.breadcrumb-header.float-start.float-lg-end(aria-label='breadcrumb')
              ol.breadcrumb
                li.breadcrumb-item
                  a(href='index.html') Acceuil
                li.breadcrumb-item.active(aria-current='page') Demande en cours
      section.section
        .col-12.col-lg-12.col-md-12
          .card.collapse-icon.accordion-icon-rotate
            .card-header
              h1.card-title.pl-1 Votre liste
            .card.collapse-header
              #headingTwo.card-header(data-bs-toggle='collapse' data-bs-target='#collapseTwo' aria-expanded='false' aria-controls='collapseTwo' role='button')
                span.collapsed.collapse-title
                  .alert.alert-warning
                    i.bi.bi-exclamation-triangle
                    |  Demande#0039DMC.
              #collapseTwo.collapse.pt-1(aria-labelledby='headingThree' data-parent='#cardAccordion')
                .card-body
                  table.table.mb-0.table-lg
                    thead
                      tr
                        th DATE
                        th DIRECTION
                        th Type de demande
                        th Action
                        th Action
                    tbody
                      tr
                        td.text-bold-500 12/08/2021
                        td Direction2
                        td.text-bold-500 Fourniture
                        td
                          a.btn.btn-primary(href='#') Modifer
                        td
                          a.btn.btn-primary(href='#') Suprimer
            .card.collapse-header
              #headingTwo.card-header(data-bs-toggle='collapse' data-bs-target='#collapsethree' aria-expanded='false' aria-controls='collapsethree' role='button')
                span.collapsed.collapse-title
                  .alert.alert-warning
                    i.bi.bi-exclamation-triangle
                    |  Demande#0035DMC.
              #collapsethree.collapse.pt-1(aria-labelledby='headingthree' data-parent='#cardAccordion')
                .card-body
                  table.table.mb-0.table-lg
                    thead
                      tr
                        th DATE
                        th DIRECTION
                        th Type de demande
                        th Action
                        th Action
                    tbody
                      tr
                        td.text-bold-500 17/08/2021
                        td Direction2
                        td.text-bold-500 Materiels
                        td
                          a.btn.btn-primary(href='#') Modifer
                        td
                          a.btn.btn-primary(href='#') Suprimer
    footer
      .footer.clearfix.mb-0.text-muted
        .float-start
          p 2021 &copy; Cosider
        .float-end
          p
            | Crafted with 
            span.text-danger
              i.bi.bi-heart
            |  by 
            a(href='#') A.choukri
script(src='vendors/perfect-scrollbar/perfect-scrollbar.min.js')
script(src='js/bootstrap.bundle.min.js')
script(src='vendors/simple-datatables/simple-datatables.js')
script.
  // Simple Datatable
  let table1 = document.querySelector('#table1');
  let dataTable = new simpleDatatables.DataTable(table1);
script(src='js/main.js')
